  "scripts": {
    "start": "expo start",
    "start:clear": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "node -e \"const fs = require('fs'); const path = require('path'); const dupPath = path.join(__dirname, 'node_modules', 'react-native-calendars', 'node_modules', 'react-native-safe-area-context'); if (fs.existsSync(dupPath)) { fs.rmSync(dupPath, { recursive: true, force: true }); console.log('Removed duplicate react-native-safe-area-context'); }\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
