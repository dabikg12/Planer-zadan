    "expo-blur": "~15.0.7",
    "expo-calendar": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "^6.0.14",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "lottie-react-native": "~7.3.1",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
